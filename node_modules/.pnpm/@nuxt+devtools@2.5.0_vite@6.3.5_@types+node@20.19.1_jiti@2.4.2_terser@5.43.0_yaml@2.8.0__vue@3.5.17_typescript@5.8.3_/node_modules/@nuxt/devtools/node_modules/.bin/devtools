#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@20.19.1_jiti@2.4.2_terser@5.43.0_yaml@2.8.0__vue@3.5.17_typescript@5.8.3_/node_modules/@nuxt/devtools/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@20.19.1_jiti@2.4.2_terser@5.43.0_yaml@2.8.0__vue@3.5.17_typescript@5.8.3_/node_modules/@nuxt/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@20.19.1_jiti@2.4.2_terser@5.43.0_yaml@2.8.0__vue@3.5.17_typescript@5.8.3_/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@20.19.1_jiti@2.4.2_terser@5.43.0_yaml@2.8.0__vue@3.5.17_typescript@5.8.3_/node_modules/@nuxt/devtools/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@20.19.1_jiti@2.4.2_terser@5.43.0_yaml@2.8.0__vue@3.5.17_typescript@5.8.3_/node_modules/@nuxt/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@20.19.1_jiti@2.4.2_terser@5.43.0_yaml@2.8.0__vue@3.5.17_typescript@5.8.3_/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.mjs" "$@"
else
  exec node  "$basedir/../../cli.mjs" "$@"
fi
